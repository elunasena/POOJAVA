
public class myBanck {

	public static void main(String[] args) {
		
		Cuenta c1 = new Cuenta();
		c1.numero = 1234;
		c1.saldo = 100.5;
		
		System.out.println(c1);
		System.out.println(c1.numero);
		
		Cuenta c2 = new Cuenta();
		
		c2 = c1;
		
		System.out.println(c2);
		System.out.println(c2.saldo);
		
	}
	
}
