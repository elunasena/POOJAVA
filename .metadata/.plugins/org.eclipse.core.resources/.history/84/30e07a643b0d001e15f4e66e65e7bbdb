
public class TestReferencia {

	public static void main(String[] args) {

		Cuenta c1 = new Cuenta();
		c1.saldo = 100;
		c1.numero = 123;
		Cuenta c2 = new Cuenta();
		c2 = c1;
		c2.numero = 234;
		c2.saldo = 50;
		
		System.out.println("REF c1: " + c1);
		System.out.println("REF c2: " + c2);
		
		System.out.println(c1.saldo);

	}

}
